Enum Dir( UP, DOWN, LEFT, RIGHT)

struct Point{
	int x;
	int y;
	Point(int x, int y): x(x), y(y){}
}
struct Snake{
	int x,y;
	list<Point> l;
	Snake(int x, int y){
		l.push(Point(x,y));
	}

	bool move(Dir d){
		switch(d)
	}
}

Snake s()